{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "outDir": "build",
    "typeRoots": ["node_modules/@types", "lib/types"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "paths": {
      "discord-status": ["lib"]
    }
  },
  "include": [
    "src/**/*.ts",
    "lib/**/*.ts",
    "scripts/**/*.ts"
  ]
}
